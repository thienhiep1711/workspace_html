#overlay
#header-sidebar.hidden-md.hidden-lg
  .left-h-sidebar
    a#sidebar-toggle(href='#')
      span#sidebar-icon
        span.line.line-1
        span.line.line-2
        span.line.line-3
  .right-h-sidebar
    .logo-mobie
      a(href='#', title='')
        img.img-fluid(src='img/logo/logo-mobie.png', alt='')
    .cogLangguage
      a(href='#', title='')
        i.ion-ios-gear-outline
      .head-lang
        a(href='#', title='')
          img(src='img/flags/lang-en.png')
          span English
        a(href='#', title='')
          img(src='img/flags/lang-vi.png')
          span Vietnamese
        a(href='#', title='')
          img(src='img/flags/lang-ja.png')
          span Japannes
        a(href='#', title='')
          img(src='img/flags/lang-kr.png')
          span Korea
    .button-call-search
      span.icon.ion-ios-search-strong
    .boxMobieSearch
      form.frMobie-search
        input#iptSearchMobie.ipt-search(type='search', placeholder='Từ khóa...')
        button.bt-search-mb(type='button')
          span.icon.ion-ios-search-strong
